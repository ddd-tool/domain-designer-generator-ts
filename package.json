    "@vitest/coverage-v8": "3.0.9",
    "@vue/reactivity": "^3.5.13",
    "npm-run-all2": "^7.0.2",
    "read-pkg": "^9.0.1",
    "vite": "^6.2.2",
    "vitest": "^3.0.9",
    "vue-fn": "0.1.0-beta.1",
    "vue-tsc": "^2.2.8"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3.5.13"
  }
