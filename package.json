{
  "name": "@ddd-tool/domain-designer-generator",
  "version": "0.1.0-beta.11",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "run-p test:dev",
    "test:once": "vitest --coverage --run",
    "test:dev": "vitest --coverage",
    "test:ui": "vitest --coverage --ui",
    "build": "run-p build:lib verify test:once && run-p build:sync-file",
    "build:lib": "vite build",
    "build:sync-file": "node ./scripts/build-sync-file.mjs",
    "verify": "vue-tsc -p ./tsconfig.build.json --composite false"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ddd-tool/domain-designer-core": "0.1.0-beta.9",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "3.0.9",
    "@vue/reactivity": "^3.5.14",
    "npm-run-all2": "^7.0.2",
    "read-pkg": "^9.0.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "vue-fn": "0.1.0-beta.1",
    "vue-tsc": "^2.2.10"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3.5.13"
  }
}
